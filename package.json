{
  "name": "lett",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn gen:api && yarn gen:routes && next dev",
    "build": "yarn gen:api && yarn gen:routes && next build",
    "start": "next start -p 7100",
    "test": "tsc test.ts && node test.js",
    "gen:api": "ts-node --project ./tsconfig.node.json ./scripts/generate-api-client.ts && yarn eslint ./api-client.ts --fix",
    "gen:routes": "ts-node --project ./tsconfig.node.json ./scripts/generate-routes.ts && yarn eslint ./routes.ts --fix",
    "lint": "yarn eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@devexperts/remote-data-ts": "^2.1.1",
    "@next/font": "13.1.1",
    "@prisma/client": "4.8.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "antd": "^5.1.2",
    "classnames": "^2.3.2",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "next": "13.1.1",
    "prisma": "^4.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "^7.8.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "8.31.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.0"
  }
}
